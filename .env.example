# Database
# Development (Mac with Postgres.app): Use your Mac username without password
# DATABASE_URL="postgresql://your-mac-username@localhost:5432/abiboard"
#
# Production (Raspberry Pi / Linux): Use postgres user with password
DATABASE_URL="postgresql://postgres:your-password@localhost:5432/abiboard"

# NextAuth
# Generate with: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"
